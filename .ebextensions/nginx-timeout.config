files:
  "/etc/nginx/conf.d/elasticbeanstalk-nginx-proxy-timeout.conf":     
    mode: "000644"
    owner: root
    group: root
    content: |
      proxy_connect_timeout       300;
      proxy_send_timeout          300;
      proxy_read_timeout          300;
      send_timeout                300;
commands:
  "00nginx-create-proxy-timeout":
    command: "if [[ ! -h /etc/nginx/conf.d/elasticbeanstalk-nginx-proxy-timeout.conf ]] ; then ln -s /etc/nginx/conf.d/elasticbeanstalk-nginx-proxy-timeout.conf /etc/nginx/conf.d/elasticbeanstalk-nginx-proxy-timeout.conf ; fi"